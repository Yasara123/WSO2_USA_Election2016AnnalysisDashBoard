CREATE TABLE TrumpPopular( text varchar(1000),count int,Owner varchar (100),Ft int,Rt int , HTag varchar(500),rank );
ALTER TABLE TrumpPopular ADD INDEX count (count);
CREATE TABLE TrumpLatest(text varchar(1000),Owner varchar (100),Ft int,Rt int , HTag varchar(500),ID varchar(500),PRIMARY KEY (ID));


